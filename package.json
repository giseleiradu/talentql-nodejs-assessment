{
  "name": "talentql-nodejs-assessment",
  "version": "1.0.0",
  "main": "src/index.js",
  "repository": "https://github.com/giseleiradu/talentql-nodejs-assessment.git",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"yarn run db:migrate && yarn run db:seed\" \"babel-node src/index.js\"",
    "run-dev": "concurrently \"yarn run db:seed\"  \"nodemon src/index.js --exec babel-node --presets @babel/preset-env\"",
    "db:migrate": "NODE_ENV=development ./node_modules/.bin/sequelize db:migrate ",
    "db:seed": "NODE_ENV=development ./node_modules/.bin/sequelize db:seed:all",
    "db:migrate:undo:all": "NODE_ENV=development ./node_modules/.bin/sequelize db:migrate:undo:all",
    "db:migrate:test": "NODE_ENV=test ./node_modules/.bin/sequelize db:migrate",
    "db:migrate:seed:test": "NODE_ENV=test ./node_modules/.bin/sequelize db:seed:all",
    "db:migrate:undo:all:test": "NODE_ENV=test ./node_modules/.bin/sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/node": "^7.13.13",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.0",
    "@babel/register": "^7.13.16",
    "@sendgrid/mail": "^7.4.2",
    "babel-node": "^0.0.1-security",
    "bcrypt": "^5.0.1",
    "cloudinary": "^1.25.1",
    "concurrently": "^6.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "global": "^4.4.0",
    "http-errors": "^1.8.0",
    "joi": "^17.4.0",
    "jsdoc": "^3.6.6",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "slugify": "^1.5.1",
    "unirest": "^0.6.0"
  },
  "devDependencies": {
    "eslint": "^7.25.0"
  }
}
